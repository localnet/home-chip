{
  "compilerOptions": {
    "lib": ["ES2024"],
    "module": "nodenext",
    "target": "es2024",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "verbatimModuleSyntax": true,
    "rewriteRelativeImportExtensions": true,
    "erasableSyntaxOnly": true
  }
}
